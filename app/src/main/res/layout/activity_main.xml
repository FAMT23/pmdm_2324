<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ut03.MainActivity">

    <SeekBar
        android:id="@+id/u1e2sbGreen"
        android:layout_width="182dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toTopOf="@+id/u1e2btbGenerar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/u1e2etxtInput"
        app:layout_constraintVertical_bias="0.418" />

    <SeekBar
        android:id="@+id/u1e2sbRed"
        android:layout_width="182dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toTopOf="@+id/u1e2sbGreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/u1e2etxtInput"
        app:layout_constraintVertical_bias="0.375" />

    <SeekBar
        android:id="@+id/u1e2sbBlue"
        android:layout_width="182dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toBottomOf="@+id/u1e2tvOutput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/u1e2sbRed"
        app:layout_constraintVertical_bias="0.323" />

    <EditText
        android:id="@+id/u1e2etxtInput"
        android:layout_width="259dp"
        android:layout_height="66dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="text"
        android:text="@string/name"
        app:layout_constraintBottom_toTopOf="@+id/u1e2tvOutput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.291"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/u1e2tvOutput"
        android:layout_width="199dp"
        android:layout_height="102dp"
        android:layout_marginBottom="44dp"
        android:text="@string/textview"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/u1e2btbGenerar"
        android:layout_width="122dp"
        android:layout_height="56dp"
        android:text="@string/generar"
        app:layout_constraintBottom_toTopOf="@+id/u1e2tvOutput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Switch
        android:id="@+id/u1e2stBlanco"
        android:layout_width="165dp"
        android:layout_height="51dp"
        android:text="@string/blanco"
        app:layout_constraintBottom_toTopOf="@+id/u1e2btbGenerar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/u1e2sbBlue"
        tools:ignore="UseSwitchCompatOrMaterialXml" />
</androidx.constraintlayout.widget.ConstraintLayout>